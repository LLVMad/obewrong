class ConversionExample is
    method AddNumbers(a: Integer, b: Real) : Real is
        return a.toReal().Plus(b)
    end
end

class TestAddNumbers() is
    var intVal : Integer(10)
    var realVal : Real(20.5)
    var result : Real
    var conv : ConversionExample
    result := conv.AddNumbers(intVal, realVal) 
end
