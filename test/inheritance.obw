class Animal is
  method speak() : String is
    return "Animal sound"
  end
end

class Dog extends Animal is
  method speak() : String is
    return "Woof"
  end
end

class Cat extends Animal is
  method speak() : String is
    return "Meow"
  end
end

class Main is
  this(argc: Integer, argv: List[String]) is
    var a : Animal()
    var d : Dog()
    var c : Cat()

    // Expected output: "Animal sound"
    Console.print(a.speak())

    // Expected output: "Woof"
    Console.print(d.speak())

    // Expected output: "Meow"
    Console.print(c.speak())
  end
end
