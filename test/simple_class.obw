module simpleclass

class MyClass is
    var value : Integer

    this(p: Integer) is
        this.value := p
    end

    method getValue() : Integer is
        return this.value
    end
end

class Main is
    this() is
        var obj : MyClass(42.Plus(2))
        var result : Integer
        result := obj.getValue()
        printf("result: %d\n", result)

        var arr : Array[Integer] := [1, 2, 3]
    end
end


