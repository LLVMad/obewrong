module arrays 

class Main is
  this() is
    var arr : Array[Integer, 5] := [1, 2, 3, 4, 5]

    printf("- integer array created: ")

    var i : Integer := 0
    for i, i.Less(5), i := i.Plus(1) is
      printf("%d ", arr[i])
    end

    // element manipulation
    arr[0] := 5
    var el : Integer := arr[0]

    printf("\n new el: %d\n", el)
  end
end
