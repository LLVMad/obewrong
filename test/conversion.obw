class ConversionExample is
    method IntToReal(a: Integer) : Real is
        return a.toReal()
    end

    method IntToBool() : Boolean is
        var a : Integer(1)
        return a.toBool()
    end

    method RealToInt(a: Real) : Integer is
        return a.ToInteger()
    end

    method 
end

